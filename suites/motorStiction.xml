<?xml version="1.0" encoding="UTF-8"?>

<suite name="Motor Stiction">
    <description>Get minimum pwm necesary to move motors</description>
    <environment>--robotname icub</environment>
	
    <!-- Motor Stiction -->
<!-- ATTENTION!!!! add --context icubEth or --context icubCan in param if you run test on ethernet robot or can robot/part -->
    <test type="dll" param="--from motorStiction_right_arm.ini"> MotorStiction </test>
    <test type="dll" param="--from motorStiction_left_arm.ini"> MotorStiction </test>
    <test type="dll" param="--from motorStiction_right_leg.ini"> MotorStiction </test>
    <test type="dll" param="--from motorStiction_left_leg.ini"> MotorStiction </test>
    <test type="dll" param="--from motorStiction_torso.ini"> MotorStiction </test>
    <test type="dll" param="--from motorStiction_head.ini"> MotorStiction </test>
    <test type="dll" param="--from motorStiction_face.ini"> MotorStiction </test> 
	
</suite>

